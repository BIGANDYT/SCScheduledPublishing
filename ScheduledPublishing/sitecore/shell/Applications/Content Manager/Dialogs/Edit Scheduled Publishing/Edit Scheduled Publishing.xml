<?xml version="1.0" encoding="utf-8" ?>
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <EditScheduledPublishing>
    <FormDialog Icon="Network/32x32/earth_time.png" Header="Edit Scheduled Publishing"
      Text="Configure existing publishing schedules.">
      <CodeBeside Type="ScheduledPublishing.sitecore.shell.Applications.ContentManager.Dialogs.EditScheduledPublishingDialog,ScheduledPublishing"/>
      <GridPanel Width="100%" Height="100%" Fixed="true">
        <Border Padding="8px 8px 4px 8px">
          <Literal Text="Modify existing publishing schedules. This affects every version in every language."/>
